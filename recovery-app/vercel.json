{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "expo export -p web",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install",
  "public": true,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
